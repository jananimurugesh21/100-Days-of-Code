class Solution {
    public boolean isValid(String s) {
        int n=s.length();
        Stack<Character> st=new Stack<>();
        if(n==1) return false;

        for(int i=0;i<n;i++){
            char brack=s.charAt(i);

            if(brack=='{' || brack=='(' || brack=='[') st.push(brack);
            else{
                if(st.isEmpty()) return false;
                char p=st.peek();
                char a;
                if(brack=='}') a='{';
                else if(brack==']') {
                    a='[';
                }
                else{ 
                    a='(';
                }

                if (a==p) st.pop();
                else
                return false;
                
            }


        }
        return st.isEmpty();
        
        
    }
}
