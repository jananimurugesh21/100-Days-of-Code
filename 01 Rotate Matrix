class Solution {
    public void rotate(int[][] mat) {
        int n=mat.length;
        int m=mat[0].length;
        int[][] arr=new int[n][m];
        for(int i=0;i<n;i++){
            for(int j=m-1;j>=0;j--){
               arr[i][m-1-j]=mat[j][i];
            }
        }

        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                mat[i][j]=arr[i][j];
            }
        }
        
    }
}
